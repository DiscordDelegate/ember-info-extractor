{
    "manifest_version": 3,
    "name": "Ember Info Extractor",
    "version": "1.0",
    "description": "A Chrome extension to extract and display Ember data from websites.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }